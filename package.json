{
  "name": "keycloak-connect",
  "version": "3.3.0",
  "description": "Keycloak Connect Middleware",
  "homepage": "http://keycloak.org",
  "main": "index.js",
  "scripts": {
    "lint": "jshint *.js stores/*.js middleware/*.js example/*.js",
    "format": "semistandard",
    "test": "tape test/unit/*.js test/*.js",
    "prepublish": "nsp check",
    "docs": "jsdoc --verbose -d docs -t ./node_modules/ink-docstrap/template -R README.md index.js ./middleware/*.js stores/*.js",
    "coverage": "istanbul cover tape -- test/unit/*.js | istanbul cover tape -- test/*.js"
  },
  "keywords": [
    "sso",
    "keycloak",
    "oauth",
    "oauth2.0",
    "authentication"
  ],
  "files": [
    "example",
    "middleware",
    "stores",
    "test",
    "index.js",
    "uuid.js"
  ],
  "author": {
    "name": "Red Hat, Inc.",
    "url": "http://keycloak.org"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "keycloak-auth-utils": "3.3.0"
  },
  "bundledDependencies": [
    "keycloak-auth-utils"
  ],
  "devDependencies": {
    "blue-tape": "^1.0.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.13.3",
    "cors": "^2.8.1",
    "coveralls": "^2.11.4",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "hogan-express": "^0.5.2",
    "ink-docstrap": "^1.3.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.3",
    "jshint": "^2.9.4",
    "keycloak-admin-client": "bucharest-gold/keycloak-admin-client",
    "keycloak-request-token": "^0.1.0",
    "nsp": "^2.6.2",
    "phantomjs-prebuilt": "^2.1.14",
    "roi": "^0.15.0",
    "selenium-webdriver": "^2.52.0",
    "semistandard": "^9.2.1",
    "server-destroy": "^1.0.1",
    "tape": "^4.6.3"
  },
  "semistandard": {
    "ignore": [
      "**/phantomjs*/"
    ]
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keycloak/keycloak-nodejs-connect.git"
  },
  "bugs": "https://github.com/keycloak/keycloak-nodejs-connect/issues"
}
